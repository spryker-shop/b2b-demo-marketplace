{% extends molecule('view-table', '@SprykerFeature:SspFileManagement') %}

{% block table %}
    {% set rows = rows | map(row => row | merge({
        actions: row.actions | map(action => action | merge({
            iconModifier: 'big',
            icon: 'download',
            title: action.label,
        })),
    })) %}

    {{ parent() }}
{% endblock %}
